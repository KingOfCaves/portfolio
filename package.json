{
  "name": "kingofcaves-portfolio",
  "version": "1.0.0",
  "description": "My portfolio.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "posthtml": {
    "plugins": {
      "posthtml-include": {},
      "posthtml-beautify": {
        "rules": {
          "indent": 4,
          "blankLines": false
        }
      }
    }
  },
  "postcss": {
    "plugins": {
      "cssnano": {},
      "autoprefixer": {}
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "clean": "rimraf ./docs/*",
    "package": "parcel ./app/index.html --open --no-source-maps --out-dir ./docs --public-url /portfolio/",
    "post": "npm-run-all clean package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KingOfCaves/wills-parcel-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KingOfCaves/parcel-boilerplate/issues"
  },
  "homepage": "https://github.com/KingOfCaves/parcel-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "autoprefixer": "^9.7.3",
    "copyfiles": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "posthtml-beautify": "^0.2.6",
    "posthtml-include": "^1.3.2",
    "sass": "^1.23.7"
  }
}
